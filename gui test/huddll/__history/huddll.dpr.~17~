library huddll;

uses
  FMX.Forms,
  System.SysUtils,
  System.Classes,
  Unit1 in 'Unit1.pas' {Form1};

{$R *.res}

procedure showme(); stdcall export;
begin
  TForm1.showme();
end;

procedure closeme(); stdcall export;
begin
  TForm1.closeme();
end;

function gethandle(): integer; stdcall export;
begin
  result := TForm1.returnHandle();
end;

function addDisplay(): integer; stdcall export;
begin
  result := Form1.addDisplayFE();
end;

exports
  showme, closeme, gethandle, addDisplay;

begin
end.
