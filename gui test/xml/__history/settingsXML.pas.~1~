{ Global Functions }

function GetmobotSettings(Doc: IXMLDocument): IXMLMobotSettingsType;
begin
  Result := Doc.GetDocBinding('mobotSettings', TXMLMobotSettingsType, TargetNamespace) as IXMLMobotSettingsType;
end;

function LoadmobotSettings(const FileName: WideString): IXMLMobotSettingsType;
begin
  Result := LoadXMLDocument(FileName).GetDocBinding('mobotSettings', TXMLMobotSettingsType, TargetNamespace) as IXMLMobotSettingsType;
end;

function NewmobotSettings: IXMLMobotSettingsType;
begin
  Result := NewXMLDocument.GetDocBinding('mobotSettings', TXMLMobotSettingsType, TargetNamespace) as IXMLMobotSettingsType;
end;

